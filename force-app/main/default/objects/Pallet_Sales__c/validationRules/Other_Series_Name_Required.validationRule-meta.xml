<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Other_Series_Name_Required</fullName>
    <active>true</active>
    <errorConditionFormula>AND (ISPICKVAL(  Pallet_Series__c , &quot;Other&quot;),ISBLANK(  Other_Series__c ))</errorConditionFormula>
    <errorDisplayField>Other_Series__c</errorDisplayField>
    <errorMessage>If series is &quot;Other&quot;, you must fill in the name of the Series.</errorMessage>
</ValidationRule>
